{
  "name": "linter-pycodestyle",
  "main": "./lib/main",
  "version": "2.0.2",
  "description": "Linter plugin for pycodestyle",
  "repository": "https://github.com/AtomLinter/linter-pycodestyle",
  "license": "MIT",
  "scripts": {
    "test": "apm test",
    "lint": "eslint ."
  },
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "atom-linter": "^8.0.0",
    "atom-package-deps": "^4.0.1"
  },
  "devDependencies": {
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "jasmine-fix": "^1.0.1"
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "keywords": [
    "atom",
    "python",
    "linter",
    "pep8",
    "pycodestyle"
  ],
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "global-require": "off",
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "atom"
          ]
        }
      ]
    },
    "globals": {
      "atom": true
    },
    "env": {
      "node": true,
      "browser": true
    }
  }
}
